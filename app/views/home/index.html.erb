<!-- Carousel
     ================================================== -->
<div id="myCarousel" class="carousel slide" data-ride="carousel">
  <!-- Indicators -->
  <ol class="carousel-indicators">
    <li data-target="#myCarousel" data-slide-to="0" class="active"></li>
    <li data-target="#myCarousel" data-slide-to="1"></li>
    <li data-target="#myCarousel" data-slide-to="2"></li>
  </ol>
  <div class="carousel-inner">
    <div class="item active">
      <%= image_tag("logoBWG.jpg") %>s
      <div class="container">
        <div class="carousel-caption">
          <p><a class="btn btn-lg btn-primary pull-right blueButton" href="#" role="button">Explore MUT</a></p>
        </div>
      </div>
    </div>
    <div class="item">
      <%= image_tag("logoBWG.jpg") %>
      <div class="container">
        <div class="carousel-caption">
          <h1>Headline 2</h1>
          <p>Headline 1 Description</p>
          <p><a class="btn btn-lg btn-primary" href="#" role="button">Headline 2 Button</a></p>
        </div>
      </div>
    </div>
    <div class="item">
      <%= image_tag("logoBWG.jpg") %>
      <div class="container">
        <div class="carousel-caption">
          <h1>Headline 3</h1>
          <p>Headline 3 Description</p>
          <p><a class="btn btn-lg btn-primary" href="#" role="button">Headline 3 Button</a></p>
        </div>
      </div>
    </div>
  </div>
  <a class="left carousel-control" href="#myCarousel" role="button" data-slide="prev"><span class="glyphicon glyphicon-chevron-left"></span></a>
  <a class="right carousel-control" href="#myCarousel" role="button" data-slide="next"><span class="glyphicon glyphicon-chevron-right"></span></a>
</div><!-- /.carousel -->

